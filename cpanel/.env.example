# Banco de dados PostgreSQL
DATABASE_URL=postgresql://usuario:senha@host:5432/database

# JWT (mesmo secret do backend Vercel)
JWT_SECRET=sua-chave-secreta-jwt

# Ambiente
NODE_ENV=production

# Google Cloud / Workspace
GOOGLE_PROJECT_ID=seu-projeto-gcp
GOOGLE_SERVICE_ACCOUNT_EMAIL=service-account@projeto.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
IMPERSONATED_USER_EMAIL=admin@seudominio.com

# Google Pub/Sub
PUBSUB_TOPIC_NAME=meet-events-topic
PUBSUB_SUBSCRIPTION_NAME=meet-events-subscription

# Webhook destino (onde enviar os dados processados)
WEBHOOK_DESTINATION_URL=https://sua-url-de-webhook.com/webhook/id

# Porta do servidor (cPanel geralmente define automaticamente)
PORT=4000

# CORS: origens permitidas (separadas por v√≠rgula)
CORS_ORIGIN=https://reuniao.lmedu.com.br

# Anthropic API Key (para o chat IA)
ANTHROPIC_API_KEY=sk-ant-...
